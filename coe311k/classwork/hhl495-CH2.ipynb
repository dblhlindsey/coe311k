    "def base10tobinary(x):\n",
    "    if x == 0:\n",
    "        return \"0\"\n",
    "    \n",
    "    bin = \"\"\n",
    "    while x > 0:\n",
    "        if (x % 2) != 0:\n",
    "            bin = \"1\" + bin\n",
    "        else:\n",
    "            bin = \"0\" + bin\n",
    "        x = x // 2\n",
    "    return bin"
   ]
